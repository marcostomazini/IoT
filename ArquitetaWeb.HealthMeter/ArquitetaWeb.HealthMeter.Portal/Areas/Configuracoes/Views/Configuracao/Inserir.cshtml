@model ArquitetaWeb.HealthMeter.Portal.Configuracoes.Configuracoes.Models.InserirConfiguracaoViewModel

@using (Html.Form((string)PageData["HeaderTitle"], (string)ViewBag.Title, labelBotao: "Salvar"))
{
    @Html.ValidationSummary(false)    
    @Html.EditorFieldFor(x => x.Entidade.MinTemperatura, Width.Two)
    @Html.EditorFieldFor(x => x.Entidade.MaxTemperatura, Width.Two)
    @Html.EditorFieldFor(x => x.Entidade.MinUmidade, Width.Two)
    @Html.EditorFieldFor(x => x.Entidade.MaxUmidade, Width.Two)
    @Html.EditorFieldFor(x => x.Entidade.TempoAtualizacao, Width.Two) 
}
