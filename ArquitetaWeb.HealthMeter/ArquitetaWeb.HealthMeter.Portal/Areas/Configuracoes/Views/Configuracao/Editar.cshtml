@model ArquitetaWeb.HealthMeter.Portal.Configuracoes.Configuracoes.Models.EditarConfiguracaoViewModel

@using (Html.Form((string)PageData["HeaderTitle"], (string)ViewBag.Title, labelBotao: "Salvar"))
{
    @Html.ValidationSummary(false)
    @Html.NewLine()
    @Html.DisplayFieldFor(x => x.Entidade.Id, Width.Two)
    @Html.DisplayFieldFor(x => x.Entidade.DataCadastro, Width.Two)
    @Html.DisplayFieldFor(x => x.Entidade.DataAlteracao, Width.Two)
    @Html.EditorFieldFor(x => x.Entidade.DescricaoSistemaExterno, Width.Three)
    @Html.NewLine()
}