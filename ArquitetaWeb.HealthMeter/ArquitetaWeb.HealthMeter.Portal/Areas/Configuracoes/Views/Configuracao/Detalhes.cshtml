@model ArquitetaWeb.HealthMeter.Portal.Configuracoes.Configuracoes.Models.DetalhesConfiguracaoViewModel

@using (Html.Form((string)PageData["HeaderTitle"], (string)ViewBag.Title, false))
{
    @Html.ValidationSummary(false)
    @Html.NewLine()
    @Html.DisplayFieldFor(x => x.Entidade.MinTemperatura, Width.Two)
    @Html.DisplayFieldFor(x => x.Entidade.MaxTemperatura, Width.Two)
    @Html.DisplayFieldFor(x => x.Entidade.MinUmidade, Width.Two)
    @Html.DisplayFieldFor(x => x.Entidade.MaxUmidade, Width.Two)
    @Html.DisplayFieldFor(x => x.Entidade.TempoAtualizacao, Width.Two)
    @Html.NewLine()
}